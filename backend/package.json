{
    "name": "backend-template",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "dev": "pnpm db:push && pnpm db:seed && cross-env NODE_ENV=development tsx watch ./src/index.ts",
        "db:seed": "cross-env NODE_ENV=development prisma db seed",
        "db:migrate": "cross-env NODE_ENV=development prisma migrate dev --name auto-migration",
        "db:push": "cross-env NODE_ENV=development prisma db push --accept-data-loss --force-reset",
        "db:generate": "cross-env NODE_ENV=development prisma generate",
        "typecheck": "tsc --noEmit",
        "build": "tsc",
        "eslint": "eslint . --ext .ts,.tsx --max-warnings 0",
        "prettier": "prettier . --write --ignore-unknown"
    },
